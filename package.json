{
  "name": "brokeaz-trader-2.0",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "login-registry": "aws codeartifact login --tool npm --repository lifehouse-registry --domain lifehouse-registry --domain-owner 556823206064 --region us-east-2",
    "preinstall": "yarn login-registry",
    "start": "nx run-many --target=serve --parallel=true",
    "start:projects": "nx run-many --target=serve --projects=$PROJECTS --parallel=true",
    "start:apis": "TZ=UTC nx run-many --target=serve --projects=booking-api,distributor-api --parallel=100",
    "lint": "nx run-many --target=lint --all",
    "lint:affected": "nx affected --target=lint --base=$BRANCH_NAME~1 --head=HEAD",
    "test": "nx run-many --target=test --all --parallel",
    "test:affected": "nx affected --target=test --base=$BRANCH_NAME~1 --head=HEAD",
    "build": "nx run-many --target=build --all --parallel",
    "build:affected": "nx affected --target=build --base=$BRANCH_NAME~1 --head=HEAD",
    "deploy": "nx run-many --target=deploy --all --parallel",
    "deploy:affected": "nx affected --target=deploy --base=$BRANCH_NAME~1 --head=HEAD"
  },
  "private": true,
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "@nestjs/apollo": "^12.0.11",
    "@nestjs/cache-manager": "^2.1.1",
    "@nestjs/common": "^10.0.2",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.2",
    "@nestjs/graphql": "^12.0.11",
    "@nestjs/platform-express": "^10.0.2",
    "axios": "^1.0.0",
    "axios-retry": "^4.0.0",
    "cache-manager": "^5.3.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "graphql": "^16.8.1",
    "luxon": "^3.4.4",
    "pino": "^8.17.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.2",
    "@nx/eslint": "17.2.4",
    "@nx/eslint-plugin": "17.2.4",
    "@nx/jest": "17.2.4",
    "@nx/js": "17.2.4",
    "@nx/nest": "17.2.4",
    "@nx/node": "17.2.4",
    "@nx/webpack": "17.2.4",
    "@nx/workspace": "17.2.4",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@svgr/webpack": "^8.0.1",
    "@swc-node/register": "~1.6.7",
    "@swc/core": "~1.3.85",
    "@types/jest": "^29.4.0",
    "@types/luxon": "^3.3.7",
    "@types/node": "18.16.9",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "~8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.4.1",
    "jest-environment-node": "^29.4.1",
    "nx": "17.2.4",
    "prettier": "^2.6.2",
    "react-refresh": "^0.10.0",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
